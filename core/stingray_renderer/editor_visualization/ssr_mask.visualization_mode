category = "Post"
render_configs = ["core/stingray_renderer/renderer"]
display_name = "SSR Mask"
activate = """
    Application.set_render_setting("debug_rendering", "true")
    Application.set_render_setting("ssr_mask_visualization", "true")
"""
deactivate = """
    Application.set_render_setting("ssr_mask_visualization", "false")
    Application.set_render_setting("debug_rendering", "false")
"""