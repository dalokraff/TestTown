category = "Material"
render_configs = ["core/stingray_renderer/renderer"]
display_name = "Specular Color"
activate = """
    Application.set_render_setting("debug_rendering", "true")
    Application.set_render_setting("gbuffer_specular_visualization", "true")
"""
deactivate = """
    Application.set_render_setting("gbuffer_specular_visualization", "false")
    Application.set_render_setting("debug_rendering", "false")
"""
